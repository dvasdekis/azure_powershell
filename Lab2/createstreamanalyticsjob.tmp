
{    
   "location":"samplelocation",  
   "properties":{    
      "sku":{    
         "name":"standard"  
      },  
      "eventsOutOfOrderPolicy":"drop",  
      "eventsOutOfOrderMaxDelayInSeconds":10,  
      "inputs":[    
         {    
            "name":"input1",  
            "properties":{    
               "type":"stream",  
               "serialization":{    
                  "type":"JSON",  
                  "properties":{    
                     "encoding":"UTF8"  
                  }  
               },  
               "datasource":{    
                  "type":"Microsoft.ServiceBus/EventHub",  
                  "properties":{    
                     "serviceBusNamespace":"sampleNamespace",  
                     "sharedAccessPolicyName":"SampleSAP",  
                     "sharedAccessPolicyKey":"sampleSAPKey",  
                     "eventHubName":"sampleEventHub",
                     "consumerGroupName":"$Default"  
                  }  
               }  
            }  
         }  
      ],  
      "transformation":{    
         "name":"transformation1",  
         "properties":{    
            "streamingUnits":1,  
            "query":"select * from samplequery"  
         }  
      },  
      "outputs":[    
         {    
            "name":"output1",  
            "properties":{    
               "datasource":{    
                  "type":"Microsoft.Sql/Server/Database",  
                  "properties":{    
                     "server":"sampleserver",  
                     "database":"sampledatabase",  
                     "table":"sampleTable",  
                     "user":"sampleuser@sqlserver",  
                     "password":"samplepassword"  
                  }  
               }  
            }  
         }  
      ]  
   }  
}