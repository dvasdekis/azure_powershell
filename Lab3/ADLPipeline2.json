{  
    "name":"ADLPipeline2",
    "properties":
    {  
        "start":"2017-03-01T00:00:00",
        "end":"2018-12-31T00:00:00",
        "description":"Pipeline copy from Azure SQL to Data Lake",
        "activities":
        [  
              {
                "name": "SQLtoDataLake",
                "description": "Copy Activity",
                "type": "Copy",
                "inputs": [
                  {
                    "name": "SQLInput"
                  }
                ],
                "outputs": [
                  {
                    "name": "ADLOutput"
                  }
                ],
                "typeProperties": {
                    "source": {
                        "type": "SqlSource"
                      },
                      "sink": {
                        "type": "AzureDataLakeStoreSink"
                      }
                },
                   "scheduler": {
                      "frequency": "Minute",
                      "interval": 15
                },
                "policy": {
                      "concurrency": 1,
                      "executionPriorityOrder": "NewestFirst",
                      "retry": 0,
                      "timeout": "01:00:00"
                }
              }
        ]
    }
}
